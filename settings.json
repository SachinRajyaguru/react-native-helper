{
  "workbench.settings.editor": "json",
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.formatOnSave": true,
  "workbench.sideBar.location": "right",
  "editor.minimap.enabled": false,
  "breadcrumbs.enabled": false,
  "editor.fontSize": 16,

  "terminal.integrated.fontSize": 16,
  "terminal.integrated.cursorStyle": "line",
  "terminal.integrated.cursorWidth": 2,
  "terminal.integrated.fontFamily": "'Ubuntu Mono', monospace",
  "terminal.integrated.letterSpacing": 0.7,
  "editor.tabCompletion": "on",
  "workbench.colorTheme": "Material Theme Darker",
  "editor.fontFamily": "Operator Mono",
  "editor.lineHeight": 24,
  "editor.fontLigatures": true,
  "explorer.decorations.badges": false,
  "workbench.colorCustomizations": {
    "editorLineNumber.foreground": "#fff9",
    "editorLineNumber.activeForeground": "#ffdf00",
    "tab.activeBorder": "#a976ca",
    "tab.activeBorderTop": "#a976ca",
    "scrollbarSlider.hoverBackground": "#fff",
    "scrollbarSlider.activeBackground": "#e3e3e3",
    "editorCursor.foreground": "#ffff00",
    "terminalCursor.foreground": "#ffff00",
    "panel.border": "#ff0000",
    "sideBar.border": "#ff0000",
    "statusBar.border": "#ff0000",
    "activityBar.border": "#ff0000",
    "titleBar.border": "#ff0000",
    "tab.border": "#ff0000",
    "window.activeBorder": "#ff0000",
    "editorGutter.addedBackground": "#ff0000",
    "editorGutter.deletedBackground": "#ff0000",
    "editorGutter.modifiedBackground": "#ff0000",
    "editorLineNumber.foreground": "#fff9",
    "editorLineNumber.activeForeground": "#ffdf00",
    "tab.activeBorder": "#ff0000",
    "tab.activeBorderTop": "#ff0000",
    "scrollbarSlider.hoverBackground": "#fff",
    "scrollbarSlider.activeBackground": "#e3e3e3"
  },
  "workbench.activityBar.visible": false
}

// workbench
  "workbench.settings.editor": "json",
  "workbench.sideBar.location": "right",
  "workbench.editor.untitled.hint": "hidden",
  "workbench.editor.enablePreviewFromCodeNavigation": false,
  "workbench.editor.enablePreviewFromQuickOpen": false,
  "workbench.startupEditor": "none",
  "workbench.layoutControl.enabled": false,
  "workbench.editor.restoreViewState": false,
  // breadcrumbs
  "breadcrumbs.enabled": false,
  //   files
  "files.autoSave": "afterDelay",
  "files.autoSaveDelay": 60000,
  "files.defaultLanguage": "javascript",
  //   editor
  "editor.formatOnSave": true,
  "editor.formatOnSaveMode": "file",
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  //   bracket
  "editor.guides.bracketPairs": "active",
  "editor.matchBrackets": "always",
  "editor.autoClosingBrackets": "always",
  "editor.guides.bracketPairsHorizontal": "active",
  //   minimap
  "editor.minimap.enabled": false,
  //   cursor
  "editor.cursorSmoothCaretAnimation": false,
  //   fontSize
  "editor.fontSize": 16,
  "editor.fontFamily": "SFMono-Regular",
  "editor.lineHeight": 28,
  "editor.fontLigatures": true,
  //   suggestion
  "javascript.suggestionActions.enabled": false,
  "editor.quickSuggestions": {
    "other": "on",
    "comments": "off",
    "strings": "off"
  },
  "editor.insertSpaces": true,
  "editor.tabCompletion": "onlySnippets",
  "editor.hover.delay": 900,
  "editor.minimap.side": "left",
  "editor.cursorBlinking": "expand",
  "editor.cursorWidth": 3,
  "editor.tabSize": 2,
  "editor.codeLens": false,
  "editor.autoIndent": "advanced",
  "editor.linkedEditing": false,
  "explorer.decorations.badges": false,
  "explorer.fileNesting.enabled": true,
  "terminal.integrated.fontSize": 18,
  "terminal.integrated.cursorStyle": "line",
  "extensions.autoCheckUpdates": false,
  "telemetry.telemetryLevel": "off",
  "search.collapseResults": "alwaysCollapse",
  "scm.diffDecorations": "none",
  "editor.largeFileOptimizations": true,
  "window.title": "${rootName}${separator}${activeEditorShort}${separator}${activeEditorMedium}",

  "editor.renderWhitespace": "boundary",
  "search.showLineNumbers": true,
  "editor.suggest.insertMode": "replace",
  "editor.detectIndentation": false,
  "editor.stickyScroll.enabled": true,
  "editor.wordBasedSuggestions": false,
  "editor.snippetSuggestions": "inline",

  "editor.codeActionsOnSave": {
    "source.fixAll": true

    // "source.organizeImports": true
  },
  "workbench.iconTheme": "eq-material-theme-icons-ocean",
  "workbench.productIconTheme": "material-product-icons",
  "workbench.colorTheme": "Community Material Theme Ocean High Contrast"
